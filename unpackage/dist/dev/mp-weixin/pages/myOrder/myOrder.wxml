<view><view class="u-tabs"><u-tabs vue-id="416a910c-1" list="{{tabList}}" bar-height="{{4}}" bar-width="{{75}}" bold="{{false}}" inactive-color="#a8aaad" active-item-style="{{activeItemStyle}}" bar-style="{{barStyle}}" is-scroll="{{false}}" current="{{current}}" data-event-opts="{{[['^change',[['change']]]]}}" bind:change="__e" bind:__l="__l"></u-tabs></view><view class="order-list"><view class="list"><block wx:if="{{list.length==0}}"><view class="onorder"><view class="text">没有相关订单</view></view></block><block wx:for="{{list}}" wx:for-item="row" wx:for-index="index" wx:key="index"><view class="row"><view class="status">{{statusText[row.status]}}</view><view class="order-info"><view class="left"><image src="{{row.image}}"></image></view><view class="right"><view class="title">{{''+row.title+''}}</view><view class="spec">{{row.spec}}</view><view class="price-number"><view class="price">{{"￥"+row.payment/100}}</view>x<view class="number">{{row.numner}}</view></view></view></view><view class="detail"><view class="number">{{"共"+row.numner+"件商品"}}</view><view class="sum">合计￥<view class="price">{{row.payment/100}}</view></view><view class="nominal">{{"(含运费 ￥"+row.freight/100+")"}}</view></view><view class="btns"><block wx:if="{{row.status=='unpaid'}}"><block><view data-event-opts="{{[['tap',[['cancelOrder',['$0'],[[['list','',index]]]]]]]}}" class="default" bindtap="__e">取消订单</view><view data-event-opts="{{[['tap',[['toPayment',['$0'],[[['list','',index]]]]]]]}}" class="pay" bindtap="__e">付款</view></block></block><block wx:if="{{row.status=='back'}}"><block><view data-event-opts="{{[['tap',[['remindDeliver',['$0'],[[['list','',index]]]]]]]}}" class="default" bindtap="__e">提醒发货</view></block></block><block wx:if="{{row.status=='unreceived'}}"><block><view data-event-opts="{{[['tap',[['showLogistics',['$0'],[[['list','',index]]]]]]]}}" class="default" bindtap="__e">查看物流</view><view class="pay">确认收货</view><view class="pay">我要退货</view></block></block><block wx:if="{{row.status=